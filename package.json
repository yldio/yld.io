{
  "name": "yld.io",
  "description": "YLD Company Website",
  "version": "1.0.0",
  "author": "YLD <hello@yld.io>",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .cache public",
    "test": "jest",
    "test:watch": "yarn run test --watch",
    "test:e2e:lambda": "jest --config jest-lambda-e2e.config.js",
    "test:e2e:ui": "testcafe chrome:headless ./tests/e2e/**/*.js --reporter spec,html:artifacts/e2e-ui/report.html --screenshots path=artifacts/e2e-ui/screenshots,takeOnFails=true",
    "lint": "run-s lint:prettier lint:eslint",
    "lint:eslint": "eslint . --cache",
    "lint:eslint:fix": "yarn run lint:eslint --fix",
    "lint:prettier": "prettier --check \"**/*.{js,ts,json,html,css,md,mdx,yml,yaml}\"",
    "lint:prettier:fix": "prettier --write \"**/*.{js,ts,json,html,css,md,mdx,yml,yaml}\"",
    "format": "yarn run lint:prettier:fix",
    "develop": "yarn run start",
    "start": "gatsby develop",
    "clean-start": "run-s clean start",
    "storybook": "start-storybook -p 6006 -s ./stories/assets",
    "storybook:static": "build-storybook -s ./stories/assets",
    "build": "run-p build:**",
    "build:app": "gatsby build",
    "build:lambda": "netlify-lambda --babelrc false build src/functions",
    "start:lambda": "netlify-lambda --babelrc false serve src/functions",
    "now-build": "yarn run storybook:static --quiet",
    "now-start": "serve storybook-static"
  },
