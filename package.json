  "scripts": {
    "test": "jest",
    "test:watch": "yarn run test --watch",
    "test:storyshots": "jest --config jest-storyshots.config.js --silent",
    "test:storyshots:update": "yarn run test:storyshots -u",
    "test:interaction": "jest --config jest-interaction.config.js",
    "test:interaction:update": "yarn run test:interaction -u",
    "test:e2e": "testcafe chrome:headless ./tests/e2e/**/*.js -S -s artifacts/screenshots --reporter html:artifacts/screenshots/report.html --assertion-timeout 10000 --selector-timeout 90000",
    "test:lighthouse": "jest --config jest-lighthouse.config.js",
    "test:unit": "jest --config jest-unit.config.js",
    "test:unit:watch": "yarn run test:unit --watch",
    "develop": "yarn start",
    "start": "gatsby develop",
    "dev:clean": "rm -rf .cache && yarn develop",
    "format": "prettier --write \"src/**/*.js\"",
    "storybook": "start-storybook -p 6006 -s ./stories/assets",
    "storybook:static": "build-storybook -s ./stories/assets",
    "build": "run-p build:**",
    "build:app": "gatsby build",
    "build:lambda": "netlify-lambda -c webpack.functions.js --babelrc false build src/functions",
    "lint": "run-p lint:**",
    "lint:js": "eslint . --cache --fix",
    "start:lambda": "netlify-lambda -c webpack.functions.js --babelrc false serve src/functions",
    "now-build": "yarn run storybook:static --quiet",
    "now-start": "serve storybook-static"
  },
