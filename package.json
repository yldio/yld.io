    "wait-on": "^6.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{js,ts,json,html,css,md,mdx,yml,yaml}": [
      "prettier --write",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      "babel-preset-gatsby"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:jest/recommended",
      "plugin:testcafe/recommended",
      "xo-space",
      "react-app",
      "prettier"
    ],
    "rules": {
      "new-cap": 0,
      "capitalized-comments": 0,
      "import/no-anonymous-default-export": 0,
      "react/prop-types": 0,
      "no-promise-executor-return": 0,
      "jsx-a11y/anchor-is-valid": 0,
      "arrow-body-style": 0,
      "camelcase": 1,
      "no-unused-vars": 2,
      "react/no-array-index-key": 1,
      "react/state-in-constructor": 1,
      "react-hooks/exhaustive-deps": 2,
      "jest/no-done-callback": 1
    },
    "env": {
      "jest": true,
      "browser": true
    }
  },
  "stylelint": {
    "processors": [
      "stylelint-processor-styled-components"
    ],
    "extends": [
      "stylelint-config-recommended",
      "stylelint-config-styled-components"
    ]
  },
  "prettier": {
    "bracketSpacing": true,
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  }
}
